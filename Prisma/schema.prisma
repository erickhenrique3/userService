generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  name      String   @default("")
  email     String   @unique
  phone     String   @unique @default("")
  password  String
  createdAt DateTime @default(now())
}

